{
  "extends": "stylelint-config-sass-guidelines",
  "rules": {
    "number-leading-zero": null,
    "selector-type-no-unknown": true,
    "selector-class-pattern": null,
    "scss/dollar-variable-pattern": null,
    "string-no-newline": true,
    "block-no-empty": true,
    "max-empty-lines": 1,
    "max-line-length": 90,
    "max-nesting-depth": 3
  },
  "ignoreFiles": ["dist/*"]
}
